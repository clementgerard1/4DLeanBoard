{
  "name": "4DLeanBoard",
  "version": "0.0.1",
  "description": "4D Lean Board",
  "private": true,
  "scripts": {
    "prod": "MODE=prod; cp -r public/* dist; webpack --config ./webpack.prod.js  & node htmlServer.js $MODE",
    "dev": "MODE=dev; webpack --config ./webpack.dev.js  & node htmlServer.js $MODE",
    "test": "MODE=test; webpack --config ./webpack.test.js  & node htmlServer.js $MODE",
    "doc": "rm -rf ./src/class/doc; ./node_modules/.bin/jsdoc -r ./src/class -d ./src/class/doc",
    "jest": "jest src/class/jest --watch",
    "commit": "npm run doc; npm run jest; MODE=prod; cp -r public/* dist; webpack --config ./webpack.prod.js; git add --all; git commit -m ",
    "deploy": "gh-pages -d dist",
    "socket": "node socketServer.js",
    "prod:windows": "xcopy public dist /s /e /c & node htmlServer.js prod | webpack --config ./webpack.prod.js",
    "dev:windows": "node htmlServer.js dev | webpack --config ./webpack.dev.js",
    "test:windows": "node htmlServer.js test | webpack --config ./webpack.test.js",
    "commit:windows": "npm run doc:windows & npm run jest & xcopy public dist /s /e /c & webpack --config ./webpack.prod.js & git add --all & git commit -m ",
    "doc:windows": "rd /s /q src\\class\\doc & node_modules\\.bin\\jsdoc -r src\\class -d src\\class\\doc"
  },
  "author": "Clément Gérard",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.10.1",
    "@babel/preset-env": "^7.9.6",
    "babel-cli": "^6.26.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "gh-pages": "^2.2.0",
    "jest": "^26.0.1",
    "jest-html-reporter": "^3.1.3",
    "jsdoc": "^3.6.4",
    "regenerator-runtime": "^0.13.5",
    "sass": "^1.26.7",
    "sass-loader": "^8.0.2",
    "socket.io": "^2.3.0",
    "style-loader": "^1.2.1",
    "svg-inline-loader": "^0.8.2",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "animate.css": "^4.1.0",
    "express": "^4.17.1",
    "forge-apis": "^0.7.3",
    "hammerjs": "^2.0.8",
    "serialize-javascript": "^4.0.0",
    "socket.io-client": "^2.3.0",
    "vue": "^2.6.11"
  }
}
